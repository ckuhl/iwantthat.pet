<!DOCTYPE html>
<html lang="en">
<head>
	<!-- Basic Page Needs -->
	<meta charset="utf-8">
	<title>{% block title %}I want that pet!{% endblock %}</title>

	<!-- CSS -->
	<link rel="stylesheet" href="{{ url_for('static', filename='css/skeleton.css') }}"/>
	{% block css %}{% endblock %}

	<!-- Favicon -->
	<link rel="shortcut icon" type="image/x-icon" href="{{ url_for('static', filename='favicon.ico') }}"/>
</head>
<body>
	<div class="container">
		{% block content %}{% endblock %}
	</div>
</body>

<!-- Javascript -->
<script src="{{ url_for('static', filename='js/jquery-3.1.1.min.js') }}"></script>
<script src="{{ url_for('static', filename='js/intercooler-1.2.1.min.js') }}"></script>
{% block js %}
	<!-- Global site tag (gtag.js) - Google Analytics -->
	<script async src="https://www.googletagmanager.com/gtag/js?id=UA-112312047-1"></script>
	<script>
window.dataLayer = window.dataLayer || [];
function gtag(){dataLayer.push(arguments);}
gtag('js', new Date());

gtag('config', 'UA-112312047-1');
	</script>

{% endblock %}
</html>
